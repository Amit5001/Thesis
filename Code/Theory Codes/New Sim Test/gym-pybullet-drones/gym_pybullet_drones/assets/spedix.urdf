    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.180 0.180 0.003"/>  <!-- 3mm carbon fiber plate -->
      </geometry>
      <material name="carbon_fiber">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
  </link>

  <!-- Lower Frame Plate -->
