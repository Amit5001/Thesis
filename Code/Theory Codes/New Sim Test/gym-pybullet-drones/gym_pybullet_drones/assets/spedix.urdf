        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
  </link>

  <!-- Front Right Arm -->
  <link name="arm_fr">
    <inertial>
      <origin xyz="0.035 0 0"/>
      <mass value="0.015"/>
      <inertia ixx="1.5e-6" ixy="0" ixz="0" iyy="3.5e-5" iyz="0" izz="3.5e-5"/>
    </inertial>

    <visual>
      <origin xyz="0.035 0 0"/>
      <geometry>
