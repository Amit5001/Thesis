      <inertia ixx="2.5e-5" ixy="0" ixz="0" iyy="2.5e-5" iyz="0" izz="4.5e-5"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.180 0.180 0.003"/>  <!-- 3mm carbon fiber plate -->
